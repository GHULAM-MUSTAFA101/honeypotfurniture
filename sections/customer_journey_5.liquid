{% comment %}
✅ Section: Pick Your Fabric & Colour (Fixed Version)
- Fabric image change on swatch click - WORKING
- Add/Remove swatches functionality
- Fully responsive with pagination
- Includes footer with three boxes
{% endcomment %}

<section class="fabric-section_main">
<div class="fabric-section">
  <div class="fabric-header">
    <h2 class="fabric-step">{{ section.settings.heading_number }}</h2>
    <h3 class="fabric-title">{{ section.settings.heading }}</h3>
    <p class="fabric-desc">{{ section.settings.subheading_1 }}</p>
    <p class="fabric-desc">{{ section.settings.subheading_2 }}</p>
  </div>

  <!-- Add New Fabric Button -->
  <div class="add-fabric-container" style="text-align: center; margin-bottom: 30px;">
    <button class="add-fabric-btn" id="addFabricBtn" style="
      background: #000;
      color: white;
      border: none;
      padding: 12px 24px;
      border-radius: 5px;
      cursor: pointer;
      font-size: 16px;
      transition: background 0.3s ease;
    ">
      + Add New Fabric
    </button>
  </div>

  <div class="fabric-grid">
    {% for block in section.blocks %}
      <div class="fabric-item" data-fabric-item data-block-id="{{ block.id }}">
        <div class="fabric-image">
          {% if block.settings.main_image != blank %}
            <img src="{{ block.settings.main_image | image_url: width: 500 }}" alt="{{ block.settings.title }}" data-fabric-main-image class="main-fabric-img">
          {% else %}
            <div class="placeholder-image">No image</div>
          {% endif %}
          
          <!-- Remove Fabric Button -->
          <button class="remove-fabric-btn" data-block-id="{{ block.id }}" style="
            position: absolute;
            top: 10px;
            right: 10px;
            background: #ff4444;
            color: white;
            border: none;
            border-radius: 50%;
            width: 30px;
            height: 30px;
            cursor: pointer;
            font-size: 16px;
            display: flex;
            align-items: center;
            justify-content: center;
            opacity: 0;
            transition: opacity 0.3s ease;
          ">×</button>
        </div>
        
        <p class="fabric-name">{{ block.settings.title }}</p>

        <div class="fabric-swatches">
          {% for i in (1..30) %}
            {% assign color_key = 'color_swatch_' | append: i %}
            {% assign image_key = 'color_image_' | append: i %}
            {% assign swatch_color = block.settings[color_key] %}
            {% assign swatch_image = block.settings[image_key] %}
            
            {% if swatch_color != blank or swatch_image != blank %}
              <div class="swatch-container">
                <span
                  class="fabric-swatch {% if swatch_image != blank %}has-image{% endif %}"
                  style="{% if swatch_color != blank %}background-color: {{ swatch_color }};{% endif %}"
                  data-swatch-image="{% if swatch_image != blank %}{{ swatch_image | image_url: width: 500 }}{% endif %}"
                  data-swatch-index="{{ i }}">
                  {% if swatch_image != blank %}
                    <img src="{{ swatch_image | image_url: width: 40 }}" alt="Swatch {{ i }}" class="swatch-thumb">
                  {% endif %}
                </span>
                <!-- Remove Swatch Button -->
                <button class="remove-swatch-btn" data-block-id="{{ block.id }}" data-swatch-index="{{ i }}" style="
                  position: absolute;
                  top: -5px;
                  right: -5px;
                  background: #ff4444;
                  color: white;
                  border: none;
                  border-radius: 50%;
                  width: 18px;
                  height: 18px;
                  font-size: 12px;
                  cursor: pointer;
                  display: none;
                  align-items: center;
                  justify-content: center;
                  line-height: 1;
                ">×</button>
              </div>
            {% endif %}
          {% endfor %}
          
          <!-- Add Swatch Button -->
          <button class="add-swatch-btn" data-block-id="{{ block.id }}" style="
            width: 28px;
            height: 28px;
            border: 2px dashed #ccc;
            border-radius: 50%;
            background: transparent;
            cursor: pointer;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 16px;
            color: #666;
            transition: all 0.3s ease;
          ">+</button>
        </div>
      </div>
    {% endfor %}
  </div>

  <!-- Pagination Controls -->
  <div class="fabric-pagination">
    <button class="pagination-btn prev-btn" disabled>Previous</button>
    <span class="pagination-info">Page <span class="current-page">1</span> of <span class="total-pages">1</span></span>
    <button class="pagination-btn next-btn">Next</button>
  </div>

  <div class="fabric-footer">
    <h4>{{ section.settings.footer_title }}</h4>
    <div class="fabric-footer-boxes">
      <div class="fabric-box">
        <span>01</span>
        <p>{{ section.settings.list_item_1 }}</p>
      </div>
      <div class="fabric-box">
        <span>02</span>
        <p>{{ section.settings.list_item_2 }}</p>
      </div>
      <div class="fabric-box">
        <span>03</span>
        <p>{{ section.settings.list_item_3 }}</p>
      </div>
    </div>
    <p class="fabric-note">{{ section.settings.footer_note }}</p>
  </div>
  </div>
</section>

<style>
  .fabric-section_main{
  background:#EDEDED
}
.fabric-section {
  padding: 60px 20px;
  text-align: left;
  max-width:1440px;
  margin:auto;
  color: #000;
}
.fabric-header {
  max-width: 700px;
  margin: 0 auto 50px;
  text-align:center;
}
.fabric-step {
  text-transform: uppercase;
  font-weight: 700;
  font-size: 35px;
  letter-spacing: 1px;
}
.fabric-title {
  font-size: 28px;
  font-weight: 500;
  margin: 10px 0 15px;
}
.fabric-desc {
  font-size: 15px;
  color: #171717;
  margin: 4px 0;
}
.fabric-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 25px;
  margin-bottom: 60px;
}
.fabric-item {
  text-align: center;
  position: relative;
}
.fabric-image {
  position: relative;
}
.fabric-image img {
  width: 100%;
  border-radius: 4px;
  object-fit: cover;
  transition: opacity 0.3s ease;
}
.fabric-item:hover .remove-fabric-btn {
  opacity: 1;
}
.fabric-name {
  font-weight: 500;
  font-size: 16px;
  margin-top: 10px;
  text-align:start;
}
.fabric-swatches {
  display: flex;
  justify-content: start;
  flex-wrap: wrap;
  gap: 6px;
  margin-top: 8px;
  max-height: 120px;
  overflow-y: auto;
  padding: 5px;
  position: relative;
}
.swatch-container {
  position: relative;
  display: inline-block;
}
.swatch-container:hover .remove-swatch-btn {
  display: flex !important;
}
.fabric-swatch {
    width: 28px;
    height: 28px;
  border-radius: 50%;
  border: 1px solid #ccc;
  cursor: pointer;
  transition: transform 0.2s ease, border 0.2s ease;
  overflow: hidden;
  flex-shrink: 0;
  display: block;
}
.fabric-swatch img.swatch-thumb {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  object-fit: cover;
  display: block;
}
.fabric-swatch:hover {
  transform: scale(1.2);
  border-color: #000;
}
.fabric-swatch.active {
  border: 2px solid #000;
  transform: scale(1.1);
}
.add-swatch-btn:hover {
  border-color: #000;
  color: #000;
  transform: scale(1.1);
}
.fabric-footer {
  text-align: center;
  margin-top: 40px;
}
.fabric-footer h4 {
     font-size: 20px;
    font-weight: 600;
    margin-bottom: 30px;
    text-transform: capitalize;
}
.fabric-footer-boxes {
  display: flex;
  justify-content: center;
  gap: 20px;
  flex-wrap: wrap;
  margin-bottom: 25px;
}
.fabric-box {
border: 1px solid #565656;
    border-radius: 10px;
    padding: 20px 30px;
    width: 267px;
    text-align: center;
    transition: all 0.3s ease;
}
.fabric-box:hover {
  border-color: #000;
  transform: translateY(-3px);
}
.fabric-box span {
  display: block;
  font-size: 20px;
  font-weight: 700;
  margin-bottom: 6px;
}
.fabric-box p {
  margin: 0;
  font-size: 15px;
}
.fabric-note {
  font-size: 13px;
  color: #171717;
}

/* Pagination for large number of fabric items */
.fabric-pagination {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 40px 0 20px;
  gap: 15px;
}

.pagination-btn {
  padding: 10px 20px;
  border: 1px solid #ddd;
  background: white;
  border-radius: 5px;
  cursor: pointer;
  transition: all 0.3s ease;
  font-family: inherit;
}

.pagination-btn:hover:not(:disabled) {
  background: #f5f5f5;
  border-color: #000;
}

.pagination-btn:disabled {
  opacity: 0.5;
  cursor: not-allowed;
}

.pagination-info {
  font-size: 14px;
  color: #666;
}

.add-fabric-btn:hover {
  background: #333 !important;
}

/* Scrollbar styling for swatches */
.fabric-swatches::-webkit-scrollbar {
  width: 6px;
  height: 6px;
}

.fabric-swatches::-webkit-scrollbar-track {
  background: #f1f1f1;
  border-radius: 10px;
}

.fabric-swatches::-webkit-scrollbar-thumb {
  background: #c1c1c1;
  border-radius: 10px;
}

.fabric-swatches::-webkit-scrollbar-thumb:hover {
  background: #a8a8a8;
}

@media (max-width: 1200px) {
  .fabric-grid { grid-template-columns: repeat(3, 1fr); }
}

@media (max-width: 992px) {
  .fabric-grid { grid-template-columns: repeat(2, 1fr); }
}
@media (max-width: 768px) {
  .fabric-grid { grid-template-columns: repeat(2, 1fr); }
  .fabric-pagination { flex-wrap: wrap; }
  .fabric-swatches {
    max-height: 100px;
    gap: 4px;
  }
  .fabric-swatch {
    width: 24px;
    height: 24px;
  }
  .add-swatch-btn {
    width: 24px;
    height: 24px;
    font-size: 14px;
  }
}
@media (max-width: 480px) {
  .fabric-grid { grid-template-columns: repeat(2, 1fr); }
  .fabric-footer-boxes { flex-direction: column; align-items: center; }
  .fabric-box { width: 100%; max-width: 300px; }
  .fabric-pagination { flex-direction: column; gap: 10px; }
  .fabric-swatches {
    max-height: 80px;
  }
}
</style>

<script>
document.addEventListener('DOMContentLoaded', function() {
  // Swatch click functionality - FIXED
  function initializeSwatchFunctionality() {
    document.querySelectorAll('[data-fabric-item]').forEach(function(item) {
      const mainImg = item.querySelector('[data-fabric-main-image]');
      const swatches = item.querySelectorAll('.fabric-swatch');

      if (!swatches.length || !mainImg) return;

      // Auto-activate first swatch with image
      const firstSwatchWithImage = Array.from(swatches).find(swatch => {
        return swatch.getAttribute('data-swatch-image') && 
               swatch.getAttribute('data-swatch-image') !== '';
      });
      
      if (firstSwatchWithImage) {
        firstSwatchWithImage.classList.add('active');
        mainImg.src = firstSwatchWithImage.getAttribute('data-swatch-image');
      }

      swatches.forEach(function(swatch) {
        swatch.addEventListener('click', function() {
          const newImg = this.getAttribute('data-swatch-image');
          console.log('Swatch clicked, image URL:', newImg); // Debug log
          
          if (!newImg || newImg === '') {
            console.log('No image URL found for this swatch');
            return;
          }

          // Remove active class from all swatches
          swatches.forEach(s => s.classList.remove('active'));
          
          // Add active class to clicked swatch
          this.classList.add('active');

          // Fade transition
          mainImg.style.opacity = '0';
          setTimeout(() => {
            mainImg.src = newImg;
            mainImg.style.opacity = '1';
            console.log('Image updated successfully'); // Debug log
          }, 200);
        });
      });
    });
  }

  // Initialize swatch functionality
  initializeSwatchFunctionality();

  // Add Fabric Functionality
  document.getElementById('addFabricBtn')?.addEventListener('click', function() {
    // This would typically open a modal or redirect to add a new fabric
    alert('Add new fabric functionality - This would open a fabric creation form');
    // In a real implementation, this would:
    // 1. Open a modal with fabric details form
    // 2. Submit the form to add new fabric block
    // 3. Refresh the section
  });

  // Remove Fabric Functionality
  document.querySelectorAll('.remove-fabric-btn').forEach(btn => {
    btn.addEventListener('click', function() {
      const blockId = this.getAttribute('data-block-id');
      const fabricItem = document.querySelector(`[data-block-id="${blockId}"]`);
      
      if (fabricItem && confirm('Are you sure you want to remove this fabric?')) {
        fabricItem.style.opacity = '0';
        fabricItem.style.transform = 'scale(0.8)';
        setTimeout(() => {
          fabricItem.remove();
          updatePagination();
        }, 300);
      }
    });
  });

  // Add Swatch Functionality
  document.querySelectorAll('.add-swatch-btn').forEach(btn => {
    btn.addEventListener('click', function() {
      const blockId = this.getAttribute('data-block-id');
      alert(`Add new swatch to fabric ${blockId} - This would open color/image picker`);
      // In real implementation:
      // 1. Open color picker/image upload
      // 2. Add new swatch to the fabric
    });
  });

  // Remove Swatch Functionality
  document.querySelectorAll('.remove-swatch-btn').forEach(btn => {
    btn.addEventListener('click', function(e) {
      e.stopPropagation(); // Prevent triggering swatch click
      const blockId = this.getAttribute('data-block-id');
      const swatchIndex = this.getAttribute('data-swatch-index');
      
      if (confirm('Are you sure you want to remove this color swatch?')) {
        const swatchContainer = this.parentElement;
        swatchContainer.style.transform = 'scale(0)';
        swatchContainer.style.opacity = '0';
        setTimeout(() => {
          swatchContainer.remove();
        }, 300);
      }
    });
  });

  // Pagination functionality for large fabric collections
  function initializePagination() {
    const fabricGrid = document.querySelector('.fabric-grid');
    const fabricItems = document.querySelectorAll('.fabric-item');
    const prevBtn = document.querySelector('.prev-btn');
    const nextBtn = document.querySelector('.next-btn');
    const currentPageSpan = document.querySelector('.current-page');
    const totalPagesSpan = document.querySelector('.total-pages');
    
    if (!fabricGrid || fabricItems.length === 0) return;
    
    const itemsPerPage = 12;
    let currentPage = 1;
    const totalPages = Math.ceil(fabricItems.length / itemsPerPage);
    
    totalPagesSpan.textContent = totalPages;
    
    function showPage(page) {
      const start = (page - 1) * itemsPerPage;
      const end = start + itemsPerPage;
      
      fabricItems.forEach((item, index) => {
        item.style.display = (index >= start && index < end) ? 'block' : 'none';
      });
      
      currentPageSpan.textContent = page;
      if (prevBtn) prevBtn.disabled = page === 1;
      if (nextBtn) nextBtn.disabled = page === totalPages;
    }
    
    prevBtn?.addEventListener('click', () => {
      if (currentPage > 1) {
        currentPage--;
        showPage(currentPage);
      }
    });
    
    nextBtn?.addEventListener('click', () => {
      if (currentPage < totalPages) {
        currentPage++;
        showPage(currentPage);
      }
    });
    
    // Show first page initially
    showPage(1);
  }

  function updatePagination() {
    initializePagination();
  }
  
  initializePagination();

  // Show/hide remove buttons on hover
  document.querySelectorAll('.fabric-item').forEach(item => {
    item.addEventListener('mouseenter', function() {
      const removeBtn = this.querySelector('.remove-fabric-btn');
      if (removeBtn) removeBtn.style.opacity = '1';
    });
    
    item.addEventListener('mouseleave', function() {
      const removeBtn = this.querySelector('.remove-fabric-btn');
      if (removeBtn) removeBtn.style.opacity = '0';
    });
  });

  document.querySelectorAll('.swatch-container').forEach(container => {
    container.addEventListener('mouseenter', function() {
      const removeBtn = this.querySelector('.remove-swatch-btn');
      if (removeBtn) removeBtn.style.display = 'flex';
    });
    
    container.addEventListener('mouseleave', function() {
      const removeBtn = this.querySelector('.remove-swatch-btn');
      if (removeBtn) removeBtn.style.display = 'none';
    });
  });
});

// Debug function to check swatch data
function debugSwatches() {
  document.querySelectorAll('[data-fabric-item]').forEach((item, index) => {
    const mainImg = item.querySelector('[data-fabric-main-image]');
    const swatches = item.querySelectorAll('.fabric-swatch');
    console.log(`Fabric ${index + 1}:`);
    console.log('Main image src:', mainImg?.src);
    swatches.forEach((swatch, swatchIndex) => {
      console.log(`Swatch ${swatchIndex + 1}:`, {
        hasImage: swatch.getAttribute('data-swatch-image'),
        imageUrl: swatch.getAttribute('data-swatch-image'),
        isActive: swatch.classList.contains('active')
      });
    });
  });
}

// Uncomment the line below to debug swatch functionality
// debugSwatches();
</script>

