        "dbb1046b-8608-4216-9c87-234fbf663d9b": {
          "type": "image-with-text",
          "disabled": true,
          "settings": {
            "layout": "image_first",
            "heading": "Tips To Arrange Your Living Room Furniture",
            "text": "<ul><li>How To Arrange Furniture.<\/li><li>Furniture With Storage.<\/li><li>Symmetrical Furniture Arrangement.<\/li><li>Make your living room feel more sociable.<\/li><li>Use the brightening sofa.<\/li><\/ul>",
            "button_label": "Sample Button",
            "button_link": "shopify:\/\/collections",
            "button_transparent": "default",
            "padding_top": 0,
            "padding_bottom": 0
          }
